    "build": "rm -rf ./build ./output && tsc",
    "start": "ts-node src/index.ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "keywords": [],
  "author": "Dzmitry Lemechko <dzmtry.lemechko@elastic.co>",
  "license": "ISC",
  "husky": {
