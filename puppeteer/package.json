      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "axios": "^0.21.4",
    "chalk": "^4.1.2",
    "json-diff": "^0.5.4",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "puppeteer": "^10.4.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "valid-url": "^1.0.9",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@types/json-diff": "^0.5.2",
