      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "axios": "0.21.1",
    "chalk": "^4.1.1",
    "json-diff": "^0.5.4",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "puppeteer": "^9.1.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "valid-url": "^1.0.9",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/json-diff": "^0.5.1",
    "@types/node": "^16.0.1",
    "@types/puppeteer": "^5.4.4",
    "@types/valid-url": "^1.0.3",
    "@types/yargs": "^17.0.2"
