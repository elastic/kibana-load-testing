{
  "batch":[
    {
      "request":{
        "params":{
          "index":"kibana_sample_data_ecommerce",
          "body":{
            "aggs":{
              "2":{
                "shard_delay":{
                  "value":"5s"
                },
                "aggs":{
                  "1":{
                    "percentiles":{
                      "field":"products.price",
                      "percents":[
                        50
                      ]
                    }
                  },
                  "3":{
                    "percentiles":{
                      "field":"products.min_price",
                      "percents":[
                        50
                      ]
                    }
                  }
                }
              }
            },
            "size":0,
            "fields":[
              {
                "field":"customer_birth_date",
                "format":"date_time"
              },
              {
                "field":"order_date",
                "format":"date_time"
              },
              {
                "field":"products.created_on",
                "format":"date_time"
              }
            ],
            "script_fields":{

            },
            "stored_fields":[
              "*"
            ],
            "runtime_mappings":{

            },
            "_source":{
              "excludes":[

              ]
            },
            "query":{
              "bool":{
                "must":[

                ],
                "filter":[
                  {
                    "range":{
                      "order_date":{
                        "format":"strict_date_optional_time",
                        "gte":"${startTime}",
                        "lte":"${endTime}"
                      }
                    }
                  }
                ],
                "should":[

                ],
                "must_not":[

                ]
              }
            }
          },
          "preference":"${preference}"
        }
      },
      "options":{
        "sessionId":"${sessionId}",
        "isRestore":false,
        "strategy":"ese",
        "isStored":false,
        "executionContext":{
          "name":"visualize",
          "url":"/app/visualize",
          "type":"application",
          "page":"editor:gauge",
          "id":"${vizId}",
          "child":{
            "type":"visualization",
            "name":"gauge",
            "id":"${vizId}",
            "description":"Gatling Gauge",
            "url":"/app/visualize#/edit/${vizId}"
          }
        }
      }
    }
  ]
}